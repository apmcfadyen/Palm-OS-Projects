"Sub object1025()

	Dim Tip as Float*6,2
	Dim Amount as Float*6,2
	Dim Total as Float*6,2
	Dim Custom as Float*6,2
	
	EnteredBillOutputLabel.Show
	EnteredBillOutputLabel.Text=str(val(BillAmtField.Text)*1)
	TipOutputLabel.Show
	TotalOutputLabel.Show
	
	Select Case ChoicesPopUp.Text
		Case ~Poor~
			Amount=val(BillAmtField.Text)
			Tip=Amount*0.1
			TipOutputLabel.Text=str(Tip)
			TotalOutputLabel.Text=str(trunc(Amount+Tip,2))
		Case ~Fair~
			Amount=val(BillAmtField.Text)
			Tip=Amount*0.15
			TipOutputLabel.Text=str(Tip)
			TotalOutputLabel.Text=str(trunc(Amount+Tip,2))
		Case ~Good~
			Amount=val(BillAmtField.Text)
			Tip=Amount*0.20
			TipOutputLabel.Text=str(Tip)
			TotalOutputLabel.Text=str(trunc(Amount+Tip,2))
		Case ~Great~
			Amount=val(BillAmtField.Text)
			Tip=Amount*0.25
			TipOutputLabel.Text=str(Tip)
			TotalOutputLabel.Text=str(trunc(Amount+Tip,2))
		Case ~Custom (%)~
			TipOutputLabel.Text=~User Defined~
			Select Case CustomTipField.Text
				Case ~~
					MsgBox ~Enter Custom Tip (%)~
				Case Else
					Amount=val(BillAmtField.Text)
					Custom=val(CustomTipField.Text)/100
					TipOutputLabel.Text=Str(Custom)
					Tip=Amount*Custom
					TipOutputLabel.Text=str(Tip)
					TotalOutputLabel.Text=str(trunc(Amount+Tip,2))
			End Select
		Case Else
			MsgBox ~Select Service Quality~
	End Select

End Sub"
